@article{pellerano2017do-extrinsic,
	abstract = {Policymakers use both extrinsic and intrinsic incentives to induce
   consumers to change behavior. This paper investigates whether the use of
   extrinsic financial incentives is complementary to intrinsic incentives,
   or whether financial incentives undermine the effect of intrinsic
   incentives. We conduct a randomized controlled trial that uses
   information interventions to residential electricity customers to test
   this question. We find that adding economic incentives to normative
   messages not only does not strengthen the effect of the latter but may
   reduce it. These results are consistent with recent theoretical work
   that suggests a tension between intrinsic motivation and extrinsic
   incentives.},
